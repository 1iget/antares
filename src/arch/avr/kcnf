choice
depends on ARCH_AVR
prompt "Select MCU variant"
  
source "antares/src/arch/avr/mcu_selection.kcnf"

endchoice

config F_CPU
int "CPU frequency in Hz"
default 8000000


config AVR_BLDR
bool "Run in bootloader section"

config AVR_BLDADDR
depends on AVR_BLDR
hex "Bootloader address"


menuconfig CONFIG_AVR_EXTRA
bool "Extra AVR Goodies"

if CONFIG_AVR_EXTRA
source "antares/src/arch/avr/vusb/kcnf"
endif



